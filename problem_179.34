package main

func main() {
	        const limit = 1e7
		        var d [limit]int
			        s := 0
				        for i := 2; i < limit; i++ {
						                for j := i; j < limit; j += i {
									                        d[j]++
												                }
														                if d[i] == d[i-1] {
																	                        s++
																				                }
																						        }
																							        print(s)
																							}
